<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edgar_avc.morena">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_ine3"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_ine3_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReferenciaActivity"
            android:label="@string/title_activity_referencia"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".views.administrador.ContainerActivity" />
        <activity
            android:name=".views.administrador.CreateAccountActivity"
            android:parentActivityName=".views.administrador.ContainerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.edgar_avc.morena.views.administrador.ContainerActivity" />
        </activity>
        <activity
            android:name=".views.administrador.CrearCasillaActivity"
            android:parentActivityName=".views.administrador.ContainerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.edgar_avc.morena.views.administrador.ContainerActivity" />
        </activity>
        <activity
            android:name=".views.administrador.AsignarCasillaActivity"
            android:parentActivityName=".views.administrador.ContainerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.edgar_avc.morena.views.administrador.ContainerActivity" />
        </activity>
        <activity
            android:name=".views.administrador.AsignarUsuarioActivity"
            android:parentActivityName=".views.administrador.ContainerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.edgar_avc.morena.views.administrador.ContainerActivity" />
        </activity>
        <activity android:name=".views.rc.ContainerActivity2" />
        <activity android:name=".views.rg.ContainerActivity3" />

        <activity android:name=".views.administrador.fragments.TotalMunicipiosActivity"
            android:parentActivityName=".views.administrador.ContainerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.edgar_avc.morena.views.administrador.ContainerActivity" />

        </activity>


    </application>

</manifest>